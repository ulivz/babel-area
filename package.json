{
  "private": true,
  "name": "babel-area",
  "description": "A collection of babel helpers, types, and plugins",
  "version": "0.1.5",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "esm/index.d.ts",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "workflow",
    "speedy",
    "speedy-js",
    "publish",
    "release",
    "npm publish"
  ],
  "scripts": {
    "bootstrap": "pnpm install --prefer-offline",
    "docs": "doctoc --title '## Table of Contents' README.md",
    "clean": "pnpm run -r clean",
    "build": "pnpm clean && pnpm run -r build",
    "test": "jest",
    "lint": "eslint --ext js,ts,tsx packages",
    "lint:fix": "npm run lint -- --fix",
    "format": "pnpm prettier --write .",
    "prepare": "husky install",
    "dev": "mono d",
    "release": "mono r --changelog --ignore-scripts --build"
  },
  "devDependencies": {
    "@nomadland/mono": "0.3.4",
    "eslint": "8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-typescript-library": "0.2.4",
    "husky": "^8.0.3",
    "@types/babel-plugin-tester": "9.0.5",
    "@types/jest": "28.1.7",
    "@types/node": "18.7.6",
    "doctoc": "2.2.0",
    "jest": "28.1.3",
    "jest-cli": "28.1.3",
    "jest-serializer-path": "0.1.15",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "strip-ansi": "6",
    "ts-jest": "28.0.8",
    "tsx": "^3.12.3",
    "typescript": "4.7.4"
  }
}
